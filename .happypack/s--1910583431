import * as types from './mutation-types';

export default {
  saveLocale: function saveLocale(_ref, locale) {
    var commit = _ref.commit;

    commit(types.SAVE_LOCALE, locale);
  },
  saveToken: function saveToken(_ref2, token) {
    var commit = _ref2.commit;

    commit(types.SAVE_TOKEN, token);
  },
  saveUser: function saveUser(_ref3, payload) {
    var commit = _ref3.commit;

    commit(types.SAVE_USER, payload.user);
    commit(types.SAVE_TOKEN, payload.token);
  },
  logout: function logout(_ref4) {
    var commit = _ref4.commit;

    commit(types.LOGOUT);
  },
  saveWarnForm: function saveWarnForm(_ref5, getWarnForm) {
    var commit = _ref5.commit;

    commit(types.SAVE_WARNFORM, getWarnForm);
  },
  saveFinancialForm: function saveFinancialForm(_ref6, financialForm) {
    var commit = _ref6.commit;

    commit(types.SAVE_FINANCIALFORM, financialForm);
  },
  saveInfoQuery: function saveInfoQuery(_ref7, infoQuery) {
    var commit = _ref7.commit;

    commit(types.SAVE_INFOQUERY, infoQuery);
  }
};