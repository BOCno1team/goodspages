import Chart from '@pages/Chart';
import EChart from '@pages/EChart';
import HNMap from '@pages/Map';
import Login from '@pages/login/login';
import IndexNew from '@pages/warnInfo/indexNew';
import FinancialInfo from '@pages/warnInfo/financialInfo';
import WarnDetail from '@pages/warnInfo/warnDetail';

var routes = [{
  path: '/chart',
  component: Chart,
  meta: {
    requireAuth: false,
    title: 'BFE-Chart',
    menuName: 'chart'
  }
}, {
  path: '/echart',
  component: EChart,
  meta: {
    requireAuth: false,
    title: 'EChart',
    menuName: 'chart'
  }
}, {
  path: '/map',
  component: HNMap,
  meta: {
    requireAuth: false,
    title: 'Map',
    menuName: 'map'
  }
}, {
  path: '/login',
  component: Login,
  meta: {
    fullScreen: true,
    requireAuth: false,
    title: '登录',
    menuName: 'audit-warning',
    topBar: false
  }
}, {
  path: '/indexNew',
  component: IndexNew,
  meta: {
    fullScreen: false,
    requireAuth: false,
    title: '审计预警平台首页',
    menuName: 'indexNew',
    topBar: true
  }
}, {
  path: '/financialInfo',
  component: FinancialInfo,
  meta: {
    fullScreen: false,
    requireAuth: false,
    title: '审计预警平台首页',
    menuName: 'financialInfo',
    topBar: false
  }
}, {
  path: '/warnDetail',
  component: WarnDetail,
  meta: {
    fullScreen: false,
    requireAuth: false,
    title: '审计预警贯穿详情',
    menuName: 'warnDetail',
    topBar: false
  }
}, {
  path: '/*',
  redirect: '/indexNew'
}];

export default routes;